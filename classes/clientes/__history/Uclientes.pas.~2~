unit Uclientes;

interface

type
  TCliente = class

  private
    FCHAVECLI   : Integer;
    FNOME       : string;
    FENDERECO   : string;
    FBAIRRO     : string;
    FCIDADE     : string;
    FUF         : Char;
    FCEP        : Char;
    FTIPOFISCAL : Char;
    FCPFCNPJ    : Char;

public
  constructor Create;

published
  property CHAVECLI: Integer read FCHAVECLI    write FCHAVECLI;
  property NOME: string      read FNOME        write FNOME;
  property ENDERECO: string  read FENDERECO    write FENDERECO;
  property BAIRRO: string    read FBAIRRO      write FBAIRRO;
  property CIDADE: string    read FCIDADE      write FCIDADE;
  property UF: Char          read FUF          write FUF;
  property CEP: Char         read FCEP         write FCEP;
  property TIPOFISCAL: Char  read FTIPOFISCAL  write FTIPOFISCAL;
  property CPFCNPJ: Char     read FCPFCNPJ     write FCPFCNPJ;
end;

implementation

{ TCliente }

constructor TCliente.Create;
begin
  inherited Create;
end;

end.
